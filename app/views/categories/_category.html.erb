<div id="<%= dom_id category %>">
  <p>
    <strong>Name:</strong>
    <%= category.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= category.description %>
  </p>

  <% category.products.each do |product| %>
    <p>
      <%= product.name %> | <%= link_to "View Product", product %>
    </p>
  <% end %>

</div>
